# aliases to improve productivity
alias email="open https://path-to-email.com/mail/inbox"
alias calendar="open https://path-to-calendar.com/"
alias projects="open https://path-to-github-projects.com/"
alias roadmap="open https://path-to-github-project-roadmap.com/"
alias repos="open https://path-to-github-repos.com/"
alias rstudio="open -a RStudio"
alias slack="open -a Slack"
alias anki="open -a Anki"

# open productivity tools
alias pomodoro="open https://pomofocus.io"

# git
alias .gs="git status"
alias .gb="git branch"
alias .gc="git commit"
alias .gls="git log --stat"
alias .gai="git add -i"

# gh cli tool
alias .il="gh issue list"
alias .ic="gh issue create"

# youtube
alias watchLater="open https://www.youtube.com/playlist\\?list=WL"
